generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum MemberAnalyticsEvent {
  join
  leave
}

model MemberAnalytics {
  time     DateTime             @id @default(now()) @db.Timestamptz(6)
  event    MemberAnalyticsEvent
  memberId String               @db.VarChar()
}
